{
  "expo": {
    "name": "GetIt",
    "slug": "GetIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "https://firebasestorage.googleapis.com/v0/b/getit-d33e8.appspot.com/o/assets%2FNotificationIcon.png?alt=media&token=0a941cf4-87a1-4445-9bf9-16bb1b6dd8c1",
          "color": "#000"
        }
      ]
    ],
    "splash": {
      "image": "https://firebasestorage.googleapis.com/v0/b/getit-d33e8.appspot.com/o/assets%2FlogoMain.png?alt=media&token=68be1839-74da-4518-bb69-9819888bf1a8",
      "backgroundColor": "#3C4456"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rakshit223.GetIt",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c4eee596-06e6-4860-a380-a9ba8e51a8e2"
      }
    }
  }
}
